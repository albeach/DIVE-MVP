# .env.example
# MongoDB Configuration
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=admin_password
MONGO_APP_USERNAME=dive25_app
MONGO_APP_PASSWORD=app_password
MONGO_EXPRESS_USERNAME=admin
MONGO_EXPRESS_PASSWORD=admin_password

# Keycloak Configuration
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin
KEYCLOAK_CLIENT_SECRET=change-me-in-production

# API Configuration
NODE_ENV=development
API_PORT=3000
JWT_SECRET=change-me-in-production
CORS_ALLOWED_ORIGINS=http://localhost:3000,https://dive25.local,https://dive25.com

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000/api  # Through Kong
NEXT_PUBLIC_KEYCLOAK_URL=http://localhost:8080
NEXT_PUBLIC_KEYCLOAK_REALM=dive25
NEXT_PUBLIC_KEYCLOAK_CLIENT_ID=dive25-frontend

# LDAP Configuration
LDAP_ADMIN_PASSWORD=admin_password
LDAP_CONFIG_PASSWORD=config_password
LDAP_READONLY_PASSWORD=readonly_password

# Kong Configuration
KONG_ADMIN_URL=http://localhost:8001
KONG_DATABASE_PASSWORD=kong_password

# Monitoring Configuration
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# Storage Configuration
STORAGE_PATH=/app/storage
TEMP_STORAGE_PATH=/app/temp
# Keycloak container configuration
#KEYCLOAK_CONTAINER_NAME=keycloak
#KEYCLOAK_HTTP_PORT=8080

# Keycloak health check configuration
#KEYCLOAK_HEALTH_TIMEOUT=60

# Skip Keycloak health checks for faster setup
#SKIP_KEYCLOAK_CHECKS=false
