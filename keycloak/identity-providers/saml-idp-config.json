{
    "alias": "nato-saml",
    "displayName": "NATO SAML",
    "providerId": "saml",
    "enabled": true,
    "updateProfileFirstLoginMode": "on",
    "trustEmail": true,
    "storeToken": false,
    "addReadTokenRoleOnCreate": false,
    "authenticateByDefault": false,
    "linkOnly": false,
    "firstBrokerLoginFlowAlias": "first broker login",
    "config": {
        "validateSignature": "true",
        "authnContextComparisonType": "exact",
        "postBindingLogout": "true",
        "singleLogoutServiceUrl": "https://nato-idp.example.com/saml/logout",
        "backchannelSupported": "false",
        "postBindingResponse": "true",
        "postBindingAuthnRequest": "true",
        "singleSignOnServiceUrl": "https://nato-idp.example.com/saml",
        "wantAuthnRequestsSigned": "true",
        "nameIDPolicyFormat": "urn:oasis:names:tc:SAML:2.0:nameid-format:persistent",
        "signatureAlgorithm": "RSA_SHA256",
        "useJwksUrl": "true",
        "xmlSigKeyInfoKeyNameTransformer": "KEY_ID",
        "syncMode": "IMPORT",
        "allowedClockSkew": "10",
        "forceAuthn": "false",
        "principalType": "SUBJECT",
        "entityId": "https://dive25.com/auth/realms/dive25",
        "principalAttribute": "urn:oasis:names:tc:SAML:2.0:attrname-format:uri"
    }
}