        "authorizationUrl": "https://us-gov-idp.example.com/oauth2/authorize",
        "jwksUrl": "https://us-gov-idp.example.com/oauth2/jwks",
        "userInfoUrl": "https://us-gov-idp.example.com/oauth2/userinfo",
        "logoutUrl": "https://us-gov-idp.example.com/oauth2/logout",
        "validateSignature": "true",
        "useJwksUrl": "true",
        "clientAuthMethod": "client_secret_basic",
        "syncMode": "IMPORT",
