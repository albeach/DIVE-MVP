POSTGRES_USER=keycloak
POSTGRES_PASSWORD=keycloak

# Keycloak configuration
KEYCLOAK_CONTAINER_NAME=dive25-staging-keycloak
KEYCLOAK_SERVICE_NAME=keycloak
KEYCLOAK_CONFIG_CONTAINER_NAME=dive25-staging-keycloak-config
